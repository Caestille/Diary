<controls:ThemableWindow2
	x:Class="Diary.MainWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:controls="clr-namespace:ModernThemables.Controls;assembly=ModernThemables"
	xmlns:viewmodels="clr-namespace:Diary.ViewModels"
	xmlns:views="clr-namespace:Diary.Views"
	Title="Diary"
	Width="1400"
	Height="800"
	FontFamily="Resources/Fonts/#Inter"
	FontSize="13"
	Icon="Resources/Icon.ico"
	RenderOptions.BitmapScalingMode="NearestNeighbor"
	RenderOptions.ClearTypeHint="Auto"
	SnapsToDevicePixels="True"
	UseLayoutRounding="True"
	VisualEdgeMode="Aliased"
	VisualTextRenderingMode="Aliased"
	WindowStartupLocation="CenterScreen">
	<views:MainView />
</controls:ThemableWindow2>
